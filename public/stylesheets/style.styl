body
  border 0
  outline 0
  font-size 100%
  font-family "Lucida Grande", Helvetica, Arial, sans-serif

/****** Navigation **********/
header
  background-color #bbb

.nav
 margin-left 50px

.nav li a
 color #000
 text-transform uppercase

.nav-pills li a:hover
 background-color #808080 !important

.nav-pills li.active a
 background-color #808080 !important

/******** Sidebar **********/
.well
 margin 40px 30px

/******** Footer **********/
.navbar-fixed-bottom
 background-color #000
 color #fff
 padding-top 5px
 padding-left 20px


/******** Table**********/

.table-striped th
 text-align left
 width 200px
 height 50px